<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>NLP2RDF Demo</display-name>
    <servlet>
        <servlet-name>NIFStanfordCore</servlet-name>
        <servlet-class>org.nlp2rdf.implementation.stanfordcore.NIFStanfordCore</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NIFStemmer</servlet-name>
        <servlet-class>org.nlp2rdf.implementation.stanfordcore.NIFStemmer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NIFOpenNLP</servlet-name>
        <servlet-class>org.nlp2rdf.implementation.opennlp.NIFOpenNLP</servlet-class>
    </servlet>
     <servlet>
        <servlet-name>NIFLexo</servlet-name>
        <servlet-class>org.nlp2rdf.implementation.stanfordcorenlp.NIFLexo</servlet-class>
    </servlet>

   
    <servlet-mapping>
        <servlet-name>NIFStanfordCore</servlet-name>
        <url-pattern>/NIFStanfordCore</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NIFStemmer</servlet-name>
        <url-pattern>/NIFStemmer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NIFOpenNLP</servlet-name>
        <url-pattern>/NIFOpenNLP</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>NIFLexo</servlet-name>
        <url-pattern>/NIFLexo</url-pattern>
    </servlet-mapping>

    
</web-app>
